{
  "Discord": {
    "Token": ""
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data.db"
  },
  "DatabaseProvider": "Sqlite",
  "Cache": {
    "CleanUpFrequency": 600000,
    "CacheProvider": [
      {
        "Type": "Memory"
      },
      {
        "Type": "File",
        "Path": "cache",
        "SaveInterval": 300000
      }
    ]
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "RitsukageBot"
    }
  }
}